{
   "name": "rsclone_backend",
   "version": "1.0.0",
   "description": "Backend for RSClone team project.",
   "main": "app.ts",
   "scripts": {
      "start": "ts-node src/config/server.ts",
      "dev": "nodemon -x ts-node src/config/server.ts",
      "lint": "eslint --ext .js,.ts --fix src/",
      "prepare": "husky install",
      "typeorm:win": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "migration:generate": "npm run typeorm:win migration:generate -- -n test_migration",
      "migration:run": "npm run typeorm:win migration:run"
   },
   "engines": {
      "npm": ">=8.1.0",
      "node": ">=16.13.0"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/AlekseyVY/RSClone_backend.git"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/AlekseyVY/RSClone_backend/issues"
   },
   "homepage": "https://github.com/AlekseyVY/RSClone_backend#readme",
   "dependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "bcrypt": "^5.1.0",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.0",
      "mongodb": "^5.1.0",
      "mysql": "^2.18.1",
      "nodemon": "^2.0.21",
      "reflect-metadata": "^0.1.13",
      "socket.io": "^4.6.1",
      "ts-node": "^10.9.1",
      "typeorm": "0.3.12",
      "typescript": "^5.0.2",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@types/jsonwebtoken": "^9.0.1",
      "@types/node": "^18.15.3",
      "@typescript-eslint/eslint-plugin": "^5.56.0",
      "@typescript-eslint/parser": "^5.56.0",
      "eslint": "^8.36.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-import-resolver-typescript": "^3.5.3",
      "eslint-plugin-import": "^2.27.5",
      "husky": "^8.0.3"
   }
}
