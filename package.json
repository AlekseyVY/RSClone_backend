{
   "name": "rsclone_backend",
   "version": "1.0.0",
   "description": "Backend for RSClone team project.",
   "main": "app.ts",
   "scripts": {
      "start": "ts-node src/config/server.ts",
      "dev": "nodemon -x ts-node src/config/server.ts",
      "lint": "eslint --ext .js,.ts --fix src/",
      "prepare": "husky install",
      "typeorm:win": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "migration:generate": "npm run typeorm:win migration:generate -- -n test_migration",
      "migration:run": "npm run typeorm:win migration:run"
   },
   "engines": {
      "npm": ">=8.1.0",
      "node": ">=16.13.0"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/AlekseyVY/RSClone_backend.git"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/AlekseyVY/RSClone_backend/issues"
   },
   "homepage": "https://github.com/AlekseyVY/RSClone_backend#readme",
   "dependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "bcrypt": "^5.0.1",
      "cors": "^2.8.5",
      "dotenv": "^10.0.0",
      "express": "^4.17.2",
      "mongodb": "^3.7.3",
      "mysql": "^2.14.1",
      "nodemon": "^2.0.15",
      "phaser": "^3.55.2",
      "reflect-metadata": "^0.1.10",
      "socket.io": "^4.4.1",
      "socket.io-client": "^4.4.1",
      "ts-node": "^10.4.0",
      "typeorm": "0.2.41",
      "typescript": "^4.5.4",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^5.8.0",
      "@typescript-eslint/parser": "^5.8.0",
      "eslint": "^8.5.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-plugin-import": "^2.25.3",
      "husky": "^7.0.4"
   }
}
